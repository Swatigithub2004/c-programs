#include <stdio.h>
int main(){
    int a,b,temp;
    printf("enter two integers:");
    scanf("%d %d",&a , &b);
    if (scanf("%d %d", &a, &b) != 2) {
        printf("Invalid input. Please enter two integers.\n");
        return 1;
    }

    while(b != 0){
        temp = b;
        b = a % b;
        a = temp;
    }
    printf("Greatest Common Divisor is %d\n",a);
    return 0;
}