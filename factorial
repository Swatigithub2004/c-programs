#include <stdio.h>
int main(){
    int num , i=1;
    unsigned long long fact = 1;
    printf("enter a number:");
    scanf("%d",&num);
    if(num<0){
        printf("factorial of negative number does't exist.\n");
    }else{
        while(i <= num){
            fact *= i ;
            i++;
        }
         printf("factorial of %d = %llu\n",num,fact);
    }
    return 0;
}