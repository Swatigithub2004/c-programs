#include <stdio.h>
int main(){
    int num, rev=0, rem, original;
    printf("enter a number:");
    scanf("%d",&num);
    original = num;
    while(num != 0){
        rem = num % 10;
        rev = rev*10 + rem;
        num /= 10;
        }
        printf("reversed number = %d\n",rev);
        if(original == rev){
          printf("it is a palindrome number.\n");
         } else{
          printf("it is not a palindronme number.\n");
        }
        return 0;
} 